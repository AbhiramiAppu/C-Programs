#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i=0;
	setbuf(stdout,NULL);
	for(i=0;i<=10;i++){
	if(i%3==0 && i%5==0){
		printf("%d -BingBong",i);
	}else{
		printf("%d",i);
	}
	}
	 /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
