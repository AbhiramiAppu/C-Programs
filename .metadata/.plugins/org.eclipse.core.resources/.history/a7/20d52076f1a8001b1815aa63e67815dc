#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int n;
	setbuf(stdout,NULL);
	printf("Enter a number : "); /* prints !!!Hello World!!! */
	scanf("%d",&n);
	f(n);
	return EXIT_SUCCESS;
}

f(int n){
	if(n<=1)
		return
	f(n-1);
	printf(n);

	f(n-1);
}
