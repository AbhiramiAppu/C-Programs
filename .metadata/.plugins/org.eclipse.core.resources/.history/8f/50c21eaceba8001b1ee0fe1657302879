#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,fib=0,num;
	setbuf(stdout,NULL);
	printf("Enter the number : "); /* prints !!!Hello World!!! */
	scanf("%d",&num);
	printf("The fibanocci series of a number is : ");
	for(i=0;i<num;i++){
		fib = i + fib;
		printf("%d ",fib);
	}
	return EXIT_SUCCESS;
}
